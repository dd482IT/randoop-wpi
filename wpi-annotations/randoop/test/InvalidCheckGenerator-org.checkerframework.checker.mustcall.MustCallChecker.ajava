package randoop.test;

import randoop.sequence.ExecutableSequence;

/**
 * Generates invalid checks.
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.mustcall.MustCallChecker")
public class InvalidCheckGenerator extends TestCheckGenerator {

    public @org.checkerframework.checker.mustcall.qual.MustCall({}) InvalidChecks generateTestChecks(@org.checkerframework.checker.mustcall.qual.MustCall({}) InvalidCheckGenerator this, @org.checkerframework.checker.mustcall.qual.MustCall({}) ExecutableSequence eseq) {
        InvalidChecks checks = new InvalidChecks();
        checks.add(new InvalidValueCheck(eseq, eseq.sequence.size() - 1));
        return checks;
    }
}
