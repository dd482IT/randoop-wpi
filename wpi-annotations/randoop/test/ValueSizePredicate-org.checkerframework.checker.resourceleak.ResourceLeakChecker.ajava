package randoop.test;

import java.util.function.Predicate;
import randoop.sequence.ExecutableSequence;
import randoop.sequence.Value;

/**
 * Returns true if the sequence returns a value that is NOT too large value.
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.resourceleak.ResourceLeakChecker")
public class ValueSizePredicate implements Predicate<ExecutableSequence> {

    /**
     * Creates a ValueSizePredicate.
     */
    @org.checkerframework.dataflow.qual.SideEffectFree
    public ValueSizePredicate() {
    }

    @org.checkerframework.checker.calledmethods.qual.EnsuresCalledMethods(value = { "#1" }, methods = { "getResult" })
    public  @org.checkerframework.checker.calledmethods.qual.CalledMethods({}) boolean test(@org.checkerframework.checker.calledmethods.qual.CalledMethods({}) ValueSizePredicate this, @org.checkerframework.checker.calledmethods.qual.CalledMethods({}) ExecutableSequence eseq) {
        return Value.lastValueSizeOk(eseq);
    }
}
