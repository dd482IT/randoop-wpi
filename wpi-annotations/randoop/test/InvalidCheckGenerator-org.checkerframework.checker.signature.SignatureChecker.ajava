package randoop.test;

import randoop.sequence.ExecutableSequence;

/**
 * Generates invalid checks.
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.signature.SignatureChecker")
public class InvalidCheckGenerator extends TestCheckGenerator {

    public @org.checkerframework.checker.signature.qual.SignatureUnknown InvalidChecks generateTestChecks(@org.checkerframework.checker.signature.qual.SignatureUnknown InvalidCheckGenerator this, @org.checkerframework.checker.signature.qual.SignatureUnknown ExecutableSequence eseq) {
        InvalidChecks checks = new InvalidChecks();
        checks.add(new InvalidValueCheck(eseq, eseq.sequence.size() - 1));
        return checks;
    }
}
