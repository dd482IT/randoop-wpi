package randoop.util.predicate;

import java.util.function.Predicate;

/**
 * A predicate that always returns true.
 *
 * @param <T> the type of the object to test
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.common.returnsreceiver.ReturnsReceiverChecker")
public class AlwaysTrue<T> implements Predicate<T> {

    /**
     * {@inheritDoc}
     *
     * @return true, always
     */
    @org.checkerframework.dataflow.qual.Pure
    public   boolean test( AlwaysTrue<T> this, T t) {
        return true;
    }
}
