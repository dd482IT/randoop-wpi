package randoop.util;

/**
 * Exception for tracking errors in logging that should result in Randoop termination.
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.mustcall.MustCallChecker")
public class RandoopLoggingError extends Error {

    private static final  @org.checkerframework.checker.mustcall.qual.MustCall({}) long serialVersionUID = -3641426773814539646L;

    @org.checkerframework.dataflow.qual.SideEffectFree
    public RandoopLoggingError(@org.checkerframework.checker.mustcall.qual.MustCall({}) String message) {
        super(message);
    }
}
