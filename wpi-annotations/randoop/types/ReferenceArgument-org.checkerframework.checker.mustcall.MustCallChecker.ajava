     @org.checkerframework.checker.mustcall.qual.MustCall({}) boolean isInstantiationOfTypeArgument(@org.checkerframework.checker.mustcall.qual.MustCall({}) ReferenceArgument this, @org.checkerframework.checker.mustcall.qual.MustCall({}) TypeArgument otherArgument) {
        if (!(otherArgument instanceof ReferenceArgument)) {
