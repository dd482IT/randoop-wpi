    public  @org.checkerframework.checker.mustcall.qual.MustCall({}) boolean contains(@org.checkerframework.checker.mustcall.qual.MustCall({}) ReferenceArgument this, @org.checkerframework.checker.mustcall.qual.MustCall({}) TypeArgument otherArgument) {
