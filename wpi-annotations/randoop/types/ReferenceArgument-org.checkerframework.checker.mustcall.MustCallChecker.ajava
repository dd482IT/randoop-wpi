    @org.checkerframework.dataflow.qual.Pure
    public  @org.checkerframework.checker.mustcall.qual.MustCall({}) boolean isGeneric(@org.checkerframework.checker.mustcall.qual.MustCall({}) ReferenceArgument this,  @org.checkerframework.checker.mustcall.qual.MustCall({}) boolean ignoreWildcards) {
