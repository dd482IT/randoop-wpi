        }
        ReferenceType otherReferenceType = ((ReferenceArgument) otherArgument).getReferenceType();
        return referenceType.isInstantiationOf(otherReferenceType);
    }

    public @org.checkerframework.checker.mustcall.qual.MustCall({}) Substitution getInstantiatingSubstitution(@org.checkerframework.checker.mustcall.qual.MustCall({}) ReferenceArgument this, @org.checkerframework.checker.mustcall.qual.MustCall({}) TypeArgument otherArgument) {
        if (!(otherArgument instanceof ReferenceArgument)) {
