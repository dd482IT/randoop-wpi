package randoop.generation;

/**
 * Used by Randoop to determine whether generation should stop.
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.common.returnsreceiver.ReturnsReceiverChecker")
public interface IStopper {

    /**
     * Returns true if generation should stop.
     *
     * @return true if generation should stop
     */
    @org.checkerframework.dataflow.qual.Pure
    boolean shouldStop( IStopper this);
}
