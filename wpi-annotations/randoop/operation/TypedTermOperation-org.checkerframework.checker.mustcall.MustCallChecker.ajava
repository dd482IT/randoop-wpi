package randoop.operation;

import java.util.List;
import randoop.sequence.Variable;
import randoop.types.Substitution;
import randoop.types.Type;
import randoop.types.TypeTuple;

/**
 * Represents operations that have no declaring class, such as literal value, cast, or array
 * creation/access/assignment.
 */
@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.mustcall.MustCallChecker")
class TypedTermOperation extends TypedOperation {

    /**
     * Creates a {@link TypedOperation} for a given operation and input and output types.
     *
     * @param operation the operation
     * @param inputTypes the input types
     * @param outputType the output type
     */
    TypedTermOperation(@org.checkerframework.checker.mustcall.qual.MustCall({}) CallableOperation operation, @org.checkerframework.checker.mustcall.qual.MustCall({}) TypeTuple inputTypes, @org.checkerframework.checker.mustcall.qual.MustCall({}) Type outputType) {
        super(operation, inputTypes, outputType, null);
    }

    @org.checkerframework.dataflow.qual.Pure
    public  @org.checkerframework.checker.mustcall.qual.MustCall({}) boolean hasWildcardTypes(@org.checkerframework.checker.mustcall.qual.MustCall({}) TypedTermOperation this) {
        return false;
    }

    @org.checkerframework.dataflow.qual.Pure
    public void appendCode(@org.checkerframework.checker.mustcall.qual.MustCall({}) TypedTermOperation this, @org.checkerframework.checker.mustcall.qual.MustCall({}) List<Variable> inputVars, @org.checkerframework.checker.mustcall.qual.MustCall({}) StringBuilder b) {
        this.getOperation().appendCode(null, getInputTypes(), getOutputType(), inputVars, b);
    }

    public @org.checkerframework.checker.mustcall.qual.MustCall({}) TypedTermOperation substitute(@org.checkerframework.checker.mustcall.qual.MustCall({}) TypedTermOperation this, @org.checkerframework.checker.mustcall.qual.MustCall({}) Substitution substitution) {
        TypeTuple inputTypes = this.getInputTypes().substitute(substitution);
        Type outputType = this.getOutputType().substitute(substitution);
        return new TypedTermOperation(this.getOperation(), inputTypes, outputType);
    }

    public @org.checkerframework.checker.mustcall.qual.MustCall({}) TypedOperation applyCaptureConversion(@org.checkerframework.checker.mustcall.qual.MustCall({}) TypedTermOperation this) {
        TypeTuple inputTypes = this.getInputTypes().applyCaptureConversion();
        Type outputType = this.getOutputType().applyCaptureConversion();
        return new TypedTermOperation(this.getOperation(), inputTypes, outputType);
    }

    @org.checkerframework.dataflow.qual.Pure
    public @org.checkerframework.checker.mustcall.qual.MustCall({}) String toParsableString(@org.checkerframework.checker.mustcall.qual.MustCall({}) TypedTermOperation this) {
        return this.getOperation().toParsableString(null, getInputTypes(), getOutputType());
    }
}
